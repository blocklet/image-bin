{
  "name": "image-bin",
  "version": "0.7.6",
  "scripts": {
    "dev": "blocklet dev",
    "build": "vite build",
    "lint": "eslint src api --ext .mjs,.js,.jsx,.ts,.tsx",
    "start": "NODE_ENV=development nodemon api/dev.js -w api",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run bundle:client && npm run bundle:api",
    "bundle:client": "npm run build",
    "bundle:api": "npm run clean && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcblock/did-connect": "^2.5.0",
    "@arcblock/ux": "^2.5.0",
    "@blocklet/sdk": "^1.8.68-beta-500af7e5",
    "@blocklet/ui-react": "^2.5.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.4",
    "@vitejs/plugin-react": "^3.1.0",
    "ahooks": "^3.7.5",
    "axios": "^0.26.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "copy-to-clipboard": "^3.3.2",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-history-api-fallback": "^2.2.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "multer": "^1.4.4",
    "nanoid": "^3.3.4",
    "pretty-bytes": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.0",
    "serve-static": "^1.15.0",
    "swiper": "^9.1.0",
    "timeago.js": "^4.0.2",
    "uppload": "^3.2.1",
    "url-join": "^4.0.1",
    "vite": "^4.1.4",
    "vite-plugin-blocklet": "^0.5.18",
    "wolfy87-eventemitter": "^5.2.9",
    "xbytes": "^1.8.0"
  },
  "devDependencies": {
    "@arcblock/eslint-config": "^0.2.3",
    "eslint": "^8.23.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30001373"
  }
}
