{
  "name": "@blocklet/uploader",
  "version": "0.0.46",
  "description": "blocklet upload component",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./middlewares": {
      "require": "./lib/middlewares.cjs",
      "import": "./lib/middlewares.mjs",
      "types": "./lib/middlewares.d.ts"
    },
    "./react": {
      "require": "./lib/react.cjs",
      "import": "./lib/react.mjs",
      "types": "./lib/react.d.ts"
    },
    "./lib/": "./lib/"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "*.d.ts"
  ],
  "scripts": {
    "coverage": "pnpm test -- --coverage",
    "test": "vitest tests",
    "build": "unbuild",
    "build:watch": "npx nodemon --ext 'ts,tsx,json,js,jsx' --exec 'pnpm run build' --ignore 'lib/*' ",
    "dev": "pnpm run build:watch",
    "prepublish": "pnpm run build",
    "prebuild:dep": "pnpm run build",
    "postinstall": "pnpm run build"
  },
  "keywords": [
    "blocklet",
    "uploader"
  ],
  "author": "arcblock <blocklet@arcblock.io> https://github.com/blocklet",
  "license": "ISC",
  "dependencies": {
    "@abtnode/cron": "^1.16.18",
    "@blocklet/constant": "^1.16.18",
    "@blocklet/sdk": "^1.16.18",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@tus/file-store": "1.0.0-beta.2",
    "@tus/server": "1.0.0-beta.7",
    "@uppy/companion": "^4.11.0",
    "@uppy/core": "^3.7.1",
    "@uppy/dashboard": "^3.7.1",
    "@uppy/drag-drop": "^3.0.3",
    "@uppy/drop-target": "^2.0.2",
    "@uppy/file-input": "^3.0.4",
    "@uppy/golden-retriever": "^3.1.1",
    "@uppy/image-editor": "^2.3.0",
    "@uppy/locales": "^3.4.0",
    "@uppy/progress-bar": "^3.0.4",
    "@uppy/react": "^3.2.1",
    "@uppy/status-bar": "^3.2.5",
    "@uppy/tus": "^3.4.0",
    "@uppy/unsplash": "^3.2.3",
    "@uppy/url": "^3.4.0",
    "@uppy/webcam": "^3.3.4",
    "ahooks": "^3.7.8",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "mime-types": "^2.1.35",
    "p-queue": "^6.6.2",
    "preact": "^10.19.2",
    "ufo": "^1.3.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@arcblock/eslint-config-ts": "^0.2.4",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.201",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/url-join": "^4.0.3",
    "@vitest/coverage-c8": "^0.31.4",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.4",
    "vitest-fetch-mock": "^0.2.2"
  }
}
